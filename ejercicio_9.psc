Algoritmo ejercicio_9
    Para numero <- 1 Hasta 5 Con Paso 1 Hacer
        Escribir "Tabla de multiplicar del ", numero, ":"

        Para i <- 1 Hasta 10 Con Paso 1 Hacer
            Escribir numero, " * ", i, " = ", numero * i
        FinPara
		
        Escribir "" 
    FinPara

FinAlgoritmo
